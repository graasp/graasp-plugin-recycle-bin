{
  "name": "graasp-plugin-recycle-bin",
  "version": "0.1.0",
  "description": "Recycle Bin Plugin",
  "main": "dist/plugin.js",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest test --forceExit --coverage --verbose",
    "test:watch": "yarn test -- --watchAll",
    "lint": "tsc --noEmit && eslint \"{src,test}/**/*.{js,ts}\"  --fix",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graasp/graasp-recycle-bin.git"
  },
  "keywords": [],
  "author": "Graasp Association",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graasp/graasp-recycle-bin/issues"
  },
  "homepage": "https://github.com/graasp/graasp-recycle-bin#readme",
  "dependencies": {
    "http-status-codes": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@types/eslint": "^8.2.2",
    "@types/graasp": "github:graasp/graasp-types",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fastify": "^3.25.3",
    "graasp-item-tags": "github:graasp/graasp-item-tags#20/deleteItemTagByItemId",
    "graasp-test": "github:graasp/graasp-test",
    "husky": "7.0.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "qs": "^6.10.2",
    "slonik": "^23.9.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "4.2.2",
    "uuid": "^8.3.2"
  },
  "packageManager": "yarn@3.2.0-rc.3"
}
